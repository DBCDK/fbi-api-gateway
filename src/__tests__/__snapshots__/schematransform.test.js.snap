  \\"\\"\\"Detailed information about the episode\\"\\"\\"
  episode: TvSeriesDetails

  \\"\\"\\"Episode titles\\"\\"\\"
  episodeTitles: [String!]

  \\"\\"\\"Detailed information about the disc\\"\\"\\"
  disc: TvSeriesDetails

