  \\"\\"\\"code for materialType\\"\\"\\"
  code: String!

  \\"\\"\\"Ths string to display\\"\\"\\"
  display: String!
}

type MaterialType {
  \\"\\"\\"jed 1.1 - the general materialtype\\"\\"\\"
  materialTypeGeneral: GeneralMaterialType!

  \\"\\"\\"jed 1.1 - the specific materialtType\\"\\"\\"
  materialTypeSpecific: SpecificMaterialType!
